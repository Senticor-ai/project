{
  "gateway": {
    "port": 18789,
    "bind": "lan",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/google/gemini-3-flash-preview"
      }
    },
    "list": [
      {
        "id": "tay",
        "workspace": "/workspace"
      }
    ]
  },
  "tools": {
    "profile": "minimal",
    "exec": {}
  }
}
