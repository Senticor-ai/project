{
  "gateway": {
    "port": 18789,
    "bind": "lan",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "controlUi": {
      "enabled": false
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        }
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/google/gemini-2.5-flash"
      },
      "imageModel": {
        "primary": "openrouter/google/gemini-2.5-flash"
      }
    },
    "list": [
      {
        "id": "openclaw",
        "workspace": "/workspace"
      }
    ]
  },
  "tools": {
    "profile": "coding",
    "exec": {
      "security": "full"
    }
  }
}
