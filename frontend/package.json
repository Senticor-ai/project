{
  "name": "project",
  "private": true,
  "version": "0.1.0",
  "license": "EUPL-1.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:stack": "bash ../scripts/dev-stack.sh",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:unit": "CI=1 vitest run --project=unit --coverage --coverage.reportsDirectory=./coverage/unit",
    "test:coverage:storybook": "STORYBOOK_TESTS=1 CI=1 vitest run --project=storybook --coverage.enabled --coverage.reportsDirectory=./coverage/storybook",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "test:e2e": "npx playwright test --config e2e/playwright.config.ts --project=chromium",
    "test:e2e:headed": "npx playwright test --config e2e/playwright.config.ts --project=chromium --headed",
    "test:e2e:llm": "npx playwright test --config e2e/playwright.config.ts --project=llm",
    "test:e2e:journey": "npx playwright test --config e2e/playwright.config.ts --project=journey",
    "test:e2e:remote": "E2E_BASE_URL=https://project.senticor.runs.onstackit.cloud npx playwright test --config e2e/playwright.config.ts --project=smoke",
    "e2e": "bash ../scripts/e2e-stack.sh",
    "e2e:open": "bash ../scripts/e2e-stack.sh --no-test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-persist-client": "^5.90.22",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.33.0",
    "idb-keyval": "^6.2.2",
    "material-symbols": "^0.40.2",
    "mermaid": "^11.12.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.1",
    "@storybook/addon-a11y": "^10.2.7",
    "@storybook/addon-docs": "^10.2.7",
    "@storybook/addon-onboarding": "^10.2.7",
    "@storybook/addon-vitest": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "ajv": "^8.17.1",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.2.7",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "msw": "^2.12.9",
    "playwright": "^1.58.1",
    "prettier": "^3.8.1",
    "remark-gfm": "^4.0.1",
    "storybook": "^10.2.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
